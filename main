void team1(){
    char name[20];
cout<<"enter team name"<<endl;
cin>>name;
 int t1run=0,i,j;
 int v,run=0,s;
 float run_rate=0;
    int n;
    cout<<" press 1........six run"<<endl;
    cout<<" press 2........for run"<<endl;
    cout<<" press 3........three run"<<endl;
    cout<<" press 4........two run"<<endl;
    cout<<" press 5........one run"<<endl;
    cout<<" press 6........zero run"<<endl;
    cout<<" press 7........no ball"<<endl;
    cout<<" press 8........no ball+one run"<<endl;
    cout<<" press 9........no ball+two run"<<endl;
    cout<<" press 10........no ball+three run"<<endl;
    cout<<" press 11........no ball+four "<<endl;
    cout<<" press 12........no ball+six"<<endl;
    cout<<" press 13........wide ball"<<endl;
    cout<<" press 14........wide ball+one"<<endl;
    cout<<" press 15........wide ball+two"<<endl;
    cout<<" press 16........wide ball+three"<<endl;
    cout<<" press 17........wide ball+four"<<endl;
   cout<<"how many overs"<<endl<<endl;
    scanf("%d",&v);
    for(i=1;i<=v;i++){
         cout<<" FOR OVER "<<i<<endl<<endl;
            s=i;
            for(j=1;j<=6;j++)
        {
           cout<<"give run"<<endl;
           cin>>n;
            if(n==1){
                cout<<" ball no:"<<j<<endl;
                cout<<"six runs"<<endl;
                run=run+6;
                cout<<"run"<<run<<endl<<endl;
            }
            else if(n==2){
            cout<<" ball no:"<<j<<endl;
            cout<<"four runs"<<endl;
                run=run+4;
          cout<<"run"<<run<<endl<<endl;
            }
            else if(n==3){
                  cout<<" ball no:"<<j<<endl;
                  cout<<"three runs"<<endl;
                run=run+3;
             cout<<"run"<<run<<endl<<endl;
            }
            else if(n==4){
                 cout<<" ball no:"<<j<<endl;
                 cout<<"two runs"<<endl;
                run=run+2;
               cout<<"run"<<run<<endl<<endl;
            }
            else if(n==5){
                 cout<<" ball no:"<<j<<endl;
                 cout<<"one run"<<endl;
                run=run+1;
                 cout<<"run"<<run<<endl<<endl;
            }
            else if(n==6){
                cout<<" ball no:"<<j<<endl;
                cout<<"zero run"<<endl;
                run=run+0;
                cout<<"run"<<run<<endl<<endl;
            }
            else if(n==7){
                 cout<<" ball no:"<<j<<endl;
                 cout<<"no ball"<<endl;
                run=run+1;
               cout<<"run"<<run<<endl<<endl;
                j--;
            }
            else if(n==8){
                 cout<<" ball no:"<<j<<endl;
                 cout<<"no ball+1 run"<<endl;
                run=run+2;
              cout<<"run"<<run<<endl<<endl;
                j--;
            }
            else if(n==9){
                 cout<<" ball no:"<<j<<endl;
                 cout<<"no ball+two runs"<<endl;
                run=run+3;
                cout<<"run"<<run<<endl<<endl;
                j--;
            }
            else if(n==10){
                 cout<<" ball no:"<<j<<endl;
                 cout<<"no ball+three runs"<<endl;
                run=run+4;
              cout<<"run"<<run<<endl<<endl;
                j--;
            }
            else if(n==11){
                 cout<<" ball no:"<<j<<endl;
                 cout<<"no ball+four"<<endl;
                run=run+5;
                  cout<<"run"<<run<<endl<<endl;
                j--;
            }
            else if(n==12){
                 cout<<" ball no:"<<j<<endl;
                 cout<<"no+six runs"<<endl;
                run=run+7;
             cout<<"run"<<run<<endl<<endl;
                j--;
            }
            else if(n==13){
                 cout<<" ball no:"<<j<<endl;
                 cout<<"wide ball"<<endl;
                run=run+1;
             cout<<"run"<<run<<endl<<endl;
                j--;
            }
            else if(n==14){
                 cout<<" ball no:"<<j<<endl;
                 cout<<"wide ball+one run"<<endl;
                run=run+2;
                  cout<<"run"<<run<<endl<<endl;
                j--;
            }
            else if(n==15){
                 cout<<" ball no:"<<j<<endl;
                 cout<<"wide ball+two runs"<<endl;
                run=run+3;
                cout<<"run"<<run<<endl<<endl;
                j--;
            }
            else if(n==16){
                 cout<<" ball no:"<<j<<endl;
                 cout<<"wide ball+three runs"<<endl;
                run=run+4;
                cout<<"run"<<run<<endl<<endl;
                j--;
            }
            else if(n==17){
                 cout<<" ball no:"<<j<<endl;
                 cout<<"wide ball+four runs"<<endl;
                run=run+5;
                cout<<"run"<<run<<endl<<endl<<endl;
                j--;
            }
else
    cout<<"out of choice"<<endl;
    }
cout<<"After"<<endl<<i<<" over"<<endl<<"run is "<< run<<endl;
    }
    t1run=run;
cout<<endl<<endl<<endl;
cout<<"total run of this match"<<t1run<<endl<<endl;
cout<<"over "<<v<<endl;
cout<<"run rate"<<t1run/v<<endl<<endl;
}
