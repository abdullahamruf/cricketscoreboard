switch(choice){
case 1: cout<<"call for team 1 captain"<<endl;
cout<<" 1 for head"<<endl;
cout<<"2 for tail"<<endl;
cin>>t;
switch(t){
    case 1: cout<<"HEAD"<<endl;
break;
    case 2: cout<<"TAIL"<<endl;
    break;
}
break;
case 2: cout<<"call for team 2 captain"<<endl;
cout<<" 1 for head"<<endl;
cout<<"2 for tail"<<endl;
cin>>r;
switch(r){
    case 1: cout<<"HEAD"<<endl;
break;
    case 2: cout<<"TAIL"<<endl;
    break;
}
break;
}
        srand(time(0));
    n = rand() ;
  if(n%2==0){
        cout<<"<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<TAIL<<<<<<<<<<<<<<<"<<endl<<endl<<endl;
  }
  else
   cout<<"<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<HEAD<<<<<<<<<<<<<<<"<<endl<<endl<<endl;
cout<<"won the toss option"<<endl;
cout<<" 1 for team 1"<<endl;
cout<<" 2 for team 2"<<endl;
