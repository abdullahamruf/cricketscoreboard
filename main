 void result(){
int t1run,t2run;
cout<<"enter team 1 run"<<endl<<endl;
cin>>t1run;
cout<<"enter team 2 run"<<endl<<endl;
cin>>t2run;
 if(t1run<t2run){
 cout<<"team1 is winner"<<endl;
 }
 else if(t1run==t2run){
    cout<<"match draw"<<endl;
 }
 else
    cout<<"team 2 is winner"<<endl;
 }
int main(){
int  n,choice,t,r;
